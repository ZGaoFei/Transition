<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhaogaofei.transitiontest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.TransitionOneActivity" />
        <activity android:name=".ui.CustomerTransitionActivity" />
        <activity android:name=".ui.OtherAnimationEffectActivity" />
        <activity android:name=".ui.transition.TransitionStartActivity" />
        <activity android:name=".ui.transition.TransitionEndActivity" />
        <activity android:name=".ui.transition.ShareElementTransitionStartActivity" />
        <activity android:name=".ui.transition.ShareElementTransitionEndActivity" />
        <activity android:name=".ui.transition.ShareElementTransitionNextActivity"></activity>
    </application>

</manifest>